<odoo>
    <record id="paperformat_custom_temp_line" model="report.paperformat">
        <field name="name">Print Receipt</field>
        <field name="default" eval="True"/>
        <field name="format">custom</field>
        <field name="page_height">100</field>
        <field name="page_width">80</field>
        <field name="orientation">Portrait</field>
        <field name="margin_top">0</field>
        <field name="margin_bottom">0</field>
        <field name="margin_left">0</field>
        <field name="margin_right">10</field>
        <field name="header_line" eval="False"/>
        <field name="header_spacing">3</field>
        <field name="dpi">80</field>
    </record>

    <record id="lines_report" model="ir.actions.report" >
       <field name="name">Lines Report</field>
       <field name="model">call.center.line</field>
       <field name="report_type">qweb-pdf</field>  <!--qweb-pdf or qweb-html  -->
       <field name="binding_model_id" ref="model_call_center_line"/>
       <field name="binding_type">report</field>  <!-- this field have to type  [ report - [defult] no wirte ]-->
       <field name="report_name">professional_call_center.report_style_template_line</field> <!-- this field take value = "addon_name.id"  this id for style template and this id must be uneiq-->
       <field name="paperformat_id" ref="professional_call_center.paperformat_custom_temp_line"></field>
    </record>



 <template id="report_style_template_line">
        <t t-call="web.html_container">
            <t t-foreach="docs" t-as="o">
                <t t-call="professional_call_center.report_lines_document2"/>
            </t>
       </t>
    </template>

