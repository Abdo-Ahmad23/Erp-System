<odoo>
    <record id="paperformat_custom_temp" model="report.paperformat">
        <field name="name">Print Receipt</field>
        <field name="default" eval="True"/>
        <field name="format">custom</field>
        <field name="page_height">2000</field>
        <field name="page_width">80</field>
        <field name="orientation">Portrait</field>
        <field name="margin_top">3</field>
        <field name="margin_bottom">3</field>
        <field name="margin_left">3</field>
        <field name="margin_right">3</field>
        <field name="header_line" eval="False"/>
        <field name="header_spacing">3</field>
        <field name="dpi">80</field>
    </record>

    <record id="data_report" model="ir.actions.report" >
       <field name="name">Data Report</field>
       <field name="model">lawyers</field>
       <field name="report_type">qweb-pdf</field>  <!--qweb-pdf or qweb-html  -->   
       <field name="binding_model_id" ref="model_lawyers"/>
       <field name="binding_type">report</field>  <!-- this field have to type  [ report - [defult] no wirte ]-->
       <field name="report_name">law_order.report_style_template</field> <!-- this field take value = "addon_name.id"  this id for style template and this id must be uneiq-->  
       <field name="paperformat_id" ref="law_order.paperformat_custom_temp"></field>
    </record>


    <template id="report_style_template">
        <t t-call="web.html_container">
            <t t-foreach="docs" t-as="o">
                <t t-call="law_order.report_lines_document1"/>
            </t>
       </t>
    </template>

