<!--            </t>-->
<!--        </t>-->
<!--    </template>-->
    <template id="report_lines_document1">
    <t t-call="web.basic_layout">
                <spane t-field="o.name"/>
                <spane t-field="o.national_id"/>

    </t>
</template>




